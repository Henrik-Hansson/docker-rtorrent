scgi_port=0.0.0.0:5001
encoding_list = UTF-8
log.open_file = "rtorrent.log", "/var/log/rtorrent.log"
log.add_output = "info", "rtorrent.log"
log.add_output = "critical", "rtorrent.log"
log.add_output = "error", "rtorrent.log"
throttle.min_peers.normal.set = 40
throttle.max_peers.normal.set = 50
throttle.min_peers.seed.set = 10
throttle.max_peers.seed.set = 50
throttle.max_uploads.set = 8
directory.default.set = /downloads
session.path.set = /home/rtorrent/.rtorrent/session
network.port_range.set = 50001-50001
network.port_random.set = no
trackers.use_udp.set = yes
dht.mode.set = disable
dht.port.set=6881
protocol.pex.set = yes
